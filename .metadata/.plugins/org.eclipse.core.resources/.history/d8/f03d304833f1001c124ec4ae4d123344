package kh.java.func;

public class info {
	public void inf() {
		// 이름 나이 성별 주소 전화번호 이메일
		String name = "이은비";
		int age = 30;
		String sex = "여";
		String addr = "행신동";
		String phoneNum = "01077478579";
		String email = "";
		System.out.printf("이름\t나이\t성별\t지역\t전화번호\t\t이메일\t\t");
		System.out.println("-------------------------------------------");
		System.out.printf("%s\t%d\t%s\t%s\t%s\t\t%s\t\t", name, age, sex, addr, phoneNum, email);
	}
}
